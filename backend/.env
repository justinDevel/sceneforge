# SceneForge Backend Configuration

# Application
DEBUG=true
ENVIRONMENT=development

# Database 
DATABASE_URL=sqlite:///./sceneforge.db

# API Keys (add your real keys here)
BRIA_API_KEY=your_key
GOOGLE_API_KEY=your_key

# CORS Origins (JSON array format)
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173", "http://localhost:8081", "http://127.0.0.1:8081"]

# File Storage
UPLOAD_DIR=./uploads

# Logging
LOG_LEVEL=INFO